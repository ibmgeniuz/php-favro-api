{
    "name": "seregazhuk/favro-api",
    "description": "A PHP wrapper for the official Favro API",
    "keywords": [
        "php",
        "lib",
        "favro"
    ],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Zhuk Sergey",
            "email": "seregazhuk88@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.2|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "seregazhuk\\Favro\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "seregazhuk\\Favro\\" : "src",
            "seregazhuk\\tests\\" : "tests"
        }
    }
}
